/* IPP user dropdown css */
/* *********************************** */


/* Override IPP user dropdown on hover */
.li-dropdown a:hover {
	background-color: #F7FBFE !important;
	font-weight: 600;
}
/* *********************************** */

.navbar-nav > .open > a, .navbar-nav > .open > a:hover, .navbar-nav > .open > a:focus {
	color: #fff;
	background-color: #4A7295 !important;
}
.navbar-inverse .navbar-nav > .active > a, .navbar-inverse .navbar-nav > .active > a:hover, .navbar-inverse .navbar-nav > .active > a:focus {
	background-color: #4A7295 !important;
	border-bottom: 4px solid white;
	color: #fff;
	font-weight: 500; /* slightly bolder than normal */
}
.dropdown-menu > .active > a, .dropdown-menu > .active > a:hover, .dropdown-menu > .active > a:focus {
	background-color: #f5f5f5;
	color: #000000;
	outline: 0 none;
	text-decoration: none;
}
/* Positioning for logout / feedback links */
#navbar {
	position: relative;
	bottom: 45px;
	right: 20px;
}
.navbar-pma-title {
	font-size: 24px;
	font-weight: 300;
	padding-left: 12px;
	position: relative;
	top: 10px;
	color: white;
}
/* main-nav - logout link */
.navbar-right {
	position: relative;
	bottom: 1px;
}
/* main-nav link titles - Supplier Overview etc. */
.txtTopNav {
	font-size: 1.2em;
}
/* #VIEW HEADERS: BizOverview | CatOverview | Scorecard View | Tools View */
/* Common styles */
/* Fix top navbar - needs to be restyled to match scorecard */
.navbar-bizoverview-fixed, .navbar-catoverview-fixed, .navbar-tools-fixed {
	background: white;
	padding-top: 30px;
	top: 100px;
	padding-bottom: 10px;
}
/* position biz stats container */
.cat-overview-stats, .supp-overview-stats {
