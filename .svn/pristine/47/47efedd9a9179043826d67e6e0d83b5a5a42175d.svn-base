/* Slider styles */
rzslider .rz-pointer {
	background-color: #0db9f0;
	border-radius: 16px;
	cursor: pointer;
	height: 20px;
	top: -8px;
	width: 32px;
	z-index: 3;
}
rzslider .rz-pointer::after {
	background: #ffffff none repeat scroll 0 0;
	border-radius: 4px;
	content: "";
	height: 8px;
	left: 12px;
	position: absolute;
	top: 6px;
	width: 8px;
}
/* see line 31 rzslider.css for slider disabled state*/
/* Don't display numeric tick value above slider */
.tick-value {
	display: none;
}
