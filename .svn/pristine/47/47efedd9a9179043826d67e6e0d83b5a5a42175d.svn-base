}
/* Styles for sparkline charts */
/* Outer container for sparkline bar charts */
.container-sparkline-supp-overview {
	position: relative;
	bottom: 20px;
}
/* div surronding supp overview & cat overview sparkline charts - sets the scroll */
.div-sparkline-scroll {
	max-height: 200px;
	overflow-x: hidden;
	overflow-y: auto;
	margin-top: 0px;
	padding-right: 38px;
}
/* Header text that appears above sparkline bar chart */
.hdr-text-sparkline-chart {
	font-size: 14px;
	font-weight: 600;
}
.sparkline-td {
	position: relative;
	top: 5px;
	font-size: 13px;
}
/* Remove table border */
.sparkline-th {
	font-size: 13px;
	font-weight: 600; /* semi bold */
	color: #837E7E;
}
/* Hidden buttons for deselect supplier and category dropdowns */
#deselect-supp, #deselect-cat {
	display: none;
}
/* Set table padding and border to 0 for sparkline charts */
#biz-overview-sparkline>tbody>tr>td, #biz-overview-sparkline>tbody>tr>th, #biz-overview-sparkline>tfoot>tr>td, #biz-overview-sparkline>tfoot>tr>th, #biz-overview-sparkline>thead>tr>td, #biz-overview-sparkline>thead>tr>th {
