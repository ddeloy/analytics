/* Top row rating display */

/* Excellent Rating */
.rating-toplevel-score-div-excellent {
	min-width: 162px;
	height: 52px;
	border: 2px solid green;
	color: green;
	float: right;
}
/* Good Rating */
.rating-toplevel-score-div-good {
	min-width: 162px;
	height: 52px;
/*	border: 2px solid #66cd00; */
	border: 2px solid #A2CD5A;
/*	color: #66cd00; */
	color:#A2CD5A;
	float: right;
}
/* Fair Rating */
