//'use strict';
// Note: As od 08-18-16 currently not being used - but good utility directive
/**
 * @ngdoc function
 * @name tpanalyticsApp.controller:SwitchController
 * @description
 * # SwitchController
 * Controller of the tpanalyticsApp
 */
angular.module('tpanalyticsApp')
    .controller('SwitchController',['$scope', function($scope) {
	 // Default to the first content item.
                $scope.content = "content-1";
 
                // I show the content item at the given index.
                $scope.showContent = function( index ) {
 
                $scope.content = ( "content-" + index );
 
                };
}]);