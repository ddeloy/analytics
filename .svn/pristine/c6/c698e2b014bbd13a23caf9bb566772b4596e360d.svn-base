<!-- This is logic ratings for top level scorecard - far right -->
    <!-- Rating Excellent Big--> 
    <div ng-if="supplierScoreInfo.RiskScore >= 85" class="rating-toplevel-score-div-excellent" style="width:161px;height:44px;border:1px solid green;">
      <div class="pull-left" align="center" style="width:51px;height:43px;background:green;color:white;font-size:28px;position:relative;top:px;">{{supplierScoreInfo.RiskScore}}</div>
      <div align="center" class="pull-left" style="width:105px;height:30px;position:relative;top:10px;color:green;">&nbsp;
        <i class="fa fa fa-trophy fsize-14"/>
        <span style="font-weight:bold;font-size:14px;">&nbsp;&nbsp;Excellent</span></div>
    </div> 
    
    <!-- Rating Good Big--> 
 <!--   <div ng-if="supplierScoreInfo.RiskScore >= 75 && supplierScoreInfo.RiskScore < 85" class="rating-toplevel-score-div-good" style="width:161px;height:44px;border:1px solid #66cd00;">
      <div class="pull-left" align="center" style="width:51px;height:43px;background:#66cd00;color:white;font-size:28px;position:relative;top:px;">{{supplierScoreInfo.RiskScore}}</div>
      <div align="center" class="pull-left" style="width:105px;height:30px;position:relative;top:10px;color:#66cd00;">&nbsp;
        <i class="fa fa-bookmark fsize-16"/>
        <span style="font-weight:bold;font-size:16px;">&nbsp;&nbsp;Good</span></div>
    </div>  -->
    
        <!-- Rating Good Big with updating palette for scorecard summary --> 
    <div ng-if="supplierScoreInfo.RiskScore >= 75 && supplierScoreInfo.RiskScore < 85" class="rating-toplevel-score-div-good" style="width:161px;height:44px;border:1px solid #A2CD5A;">
      <div class="pull-left" align="center" style="width:51px;height:43px;background:#A2CD5A;color:white;font-size:28px;position:relative;top:px;">{{supplierScoreInfo.RiskScore}}</div>
      <div align="center" class="pull-left" style="width:105px;height:30px;position:relative;top:10px;color:#A2CD5A;">&nbsp;
        <i class="fa fa-bookmark fsize-16"/>
        <span style="font-weight:bold;font-size:16px;">&nbsp;&nbsp;Good</span></div>
    </div> 

    
    <!-- Rating Fair Big--> 
    <div ng-if="supplierScoreInfo.RiskScore >= 65 && supplierScoreInfo.RiskScore < 75" class="rating-toplevel-score-div-fair" style="width:161px;height:44px;border:1px solid #ffd700;">
      <div class="pull-left" align="center" style="width:51px;height:43px;background:#ffd700;color:white;font-size:28px;position:relative;top:px;">{{supplierScoreInfo.RiskScore}}</div>
      <div align="center" class="pull-left" style="width:105px;height:30px;position:relative;top:10px;color:#ffd700;">&nbsp;
        <fa class="fa fa-adjust fsize-16"/>
        <span style="font-weight:bold;font-size:16px;">&nbsp;&nbsp;Fair</span></div>
    </div>  
    
    <!-- Rating Poor Big--> 
    <div ng-if="supplierScoreInfo.RiskScore >= 55 && supplierScoreInfo.RiskScore < 65 " class="rating-toplevel-score-div-poor" style="width:161px;height:44px;border:1px solid #ff8c00;">
      <div class="pull-left" align="center" style="width:51px;height:43px;background:#ff8c00;color:white;font-size:28px;position:relative;top:px;">{{supplierScoreInfo.RiskScore}}</div>
      <div align="center" class="pull-left" style="width:105px;height:30px;position:relative;top:10px;color:#ff8c00;">&nbsp;
        <fa class="fa fa-flag fsize-16"/>
        <span style="font-weight:bold;font-size:16px;">&nbsp;&nbsp;Poor</span></div>
    </div> 
    
    <!-- Rating Terrible Big--> 
    <div ng-if="supplierScoreInfo.RiskScore < 55" class="rating-toplevel-score-div-terrible" style="width:161px;height:44px;border:1px solid #b22222;">
      <div class="pull-left" align="center" style="width:51px;height:43px;background:#b22222;color:white;font-size:28px;position:relative;top:px;">{{supplierScoreInfo.RiskScore}}</div>
      <div align="center" class="pull-left" style="width:105px;height:30px;position:relative;top:10px;color:#b22222;">&nbsp;
        <i class="fa fa-bookmark fsize-16"/>
        <span style="font-weight:bold;font-size:16px;">&nbsp;&nbsp;Terrible</span></div>
    </div>
    
   
    <span style="padding-right:10px;padding-top:12px;" class="pointer pull-right" data-placement="left" title="Score Ranges" html="true" 
                                data-content="
                                <div style='width:221px;height:160px;'>
                                   <img src='images/fa/scorecard-tooltip.png'/>
                                    </div>
                                "
                                data-trigger="hover" bs-popover><i class="fa fa-question-circle icon-ontarget-small"></i>
                                </span>  